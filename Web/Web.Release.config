<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <system.net>
    <mailSettings>
      <smtp>
        <network host="#{SmtpHost}" userName="#{SmtpUserName}" password="#{SmtpPassword}" xdt:Transform="SetAttributes" />
      </smtp>
    </mailSettings>
  </system.net>

  <system.webServer>
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="1.00:00:00" xdt:Transform="SetAttributes" />
    </staticContent>
  </system.webServer>

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
  
</configuration>