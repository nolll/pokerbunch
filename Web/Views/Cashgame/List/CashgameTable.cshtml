@model Web.Models.CashgameModels.List.CashgameListTableModel
<table class="cashgames table-list">
    <thead>
    <tr>
        <th class="title date">Date</th>
        <th class="title">Players</th>
        <th class="title">Location</th>
        <th class="title">Duration</th>
        <th class="title">Turnover</th>
        <th class="title">Average Buyin</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var listItemModel in Model.ListItemModels)
        {
            @Html.Partial("List/CashgameTableItem", listItemModel);
        }
    </tbody>
</table>