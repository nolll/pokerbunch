@model Web.Models.CashgameModels.End.EndGamePageModel

@{
    Layout = "~/Views/Layouts/OneColumn.cshtml";
}

<div class="block gutter">
    <h2>End Game</h2>
</div>
<div class="block gutter">

    @Html.Partial("~/Views/Forms/Errors.cshtml")

    <form method="post">
        <fieldset>
            <div>
                All done?
            </div>
            <div class="buttons">
                <button type="submit" class="action">Yes</button>
                <button data-require="cancel-button">Cancel</button>
            </div>
        </fieldset>
    </form>
    
</div>