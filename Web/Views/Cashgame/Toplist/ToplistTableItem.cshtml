@model Web.Models.CashgameModels.Toplist.CashgameToplistTableItemModel

<tr>
    <td class="rank base-column">@Model.Rank.</td>
    <td class="name base-column">
        <a href="@Model.PlayerUrl">@Model.Name</a>
    </td>
    <td class="amount result @Model.ResultClass @Model.ResultSortClass">@Model.TotalResult</td>
    <td class="amount @Model.BuyinSortClass">@Model.Buyin</td>
    <td class="amount @Model.CashoutSortClass">@Model.Cashout</td>
    <td class="gametime @Model.GameTimeSortClass">@Model.GameTime</td>
    <td class="gametime @Model.GameCountSortClass">@Model.GameCount</td>
    <td class="winrate @Model.WinRateSortClass">@Model.WinRate</td>
</tr>