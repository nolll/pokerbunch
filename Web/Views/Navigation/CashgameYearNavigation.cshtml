@model Web.Models.NavigationModels.CashgameYearNavigationModel
@if (!Model.IsEmpty)
{
    <nav class="heading-nav year-nav" data-require="heading-nav"><span>@Model.Selected</span><ul>
        @foreach (var yearModel in Model.YearModels)
        {
            <li><a href="@yearModel.Url.Relative">@yearModel.Text</a></li>
        }                                                                                          
    </ul></nav>
}