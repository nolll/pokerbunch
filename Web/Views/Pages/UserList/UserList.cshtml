@using Web.Models.PageBaseModels
@model Web.Models.UserModels.List.UserListPageModel

@{
    Layout = PageLayout.OneColumn;
}

<div class="block gutter">
	<h1>Users</h1>
</div>
<div class="block gutter">

    <ul class="users simple list">
        @foreach(var userModel in Model.UserModels)
        {
            <li>
                @Html.Partial("UserList/Item", userModel)
            </li>
        }
    </ul>
</div>