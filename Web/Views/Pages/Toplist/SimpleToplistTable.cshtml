@model Web.Models.CashgameModels.Toplist.ToplistTableModel

<table class="table-list">
    <thead>
        <tr>
            <th class="table-list__column-header"></th>
            <th class="table-list__column-header"><span class="table-list__column-header__content">Player</span></th>
            @Html.Partial("Toplist/ToplistTableColumn", Model.ColumnsModel.ResultColumn)
        </tr>
    </thead>
    <tbody class="list">
        @foreach(var itemModel in Model.ItemModels)
        {
            @Html.Partial("Toplist/SimpleToplistTableItem", itemModel)
        }
    </tbody>
</table>